{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "width": {
      "type": [
        "number",
        "string"
      ]
    },
    "height": {
      "type": [
        "number",
        "string"
      ]
    },
    "style": {
      "$ref": "#/definitions/StyleConfig"
    },
    "bgColor": {
      "type": "string"
    },
    "isStorybook": {
      "type": "boolean"
    },
    "category": {
      "$ref": "#/definitions/DynamicBarChartCategory"
    }
  },
  "additionalProperties": false,
  "required": [
    "height",
    "width"
  ],
  "definitions": {
    "StyleConfig": {
      "type": "object"
    },
    "DynamicBarChartCategory": {
      "type": "string",
      "enum": [
        "Drilldown",
        "LargeScale",
        "Race",
        "Dynamic",
        "Animation",
        "Finance"
      ]
    }
  }
}